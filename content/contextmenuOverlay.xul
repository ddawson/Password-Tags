<!DOCTYPE overlay SYSTEM "chrome://passwordtags/locale/contextmenuOverlay.dtd">

<overlay id="pwdtagsContextMenuOverlay"
         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script type="application/javascript" src="contextmenuOverlay.js"/>

  <menupopup id="contentAreaContextMenu">
    <menu id="passwordtags-fillbytags" insertbefore="context-sep-selectall"
          label="&fillbypwdtags.label;" accesskey="&fillbypwdtags.accesskey;"
          tooltiptext="&fillbypwdtags.tooltip;">
      <menupopup id="passwordtags-fillbytags-popup"/>
    </menu>
  </menupopup>
</overlay>
